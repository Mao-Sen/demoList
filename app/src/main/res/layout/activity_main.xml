<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/mapGaoDe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="进入高德地图"/>

    <Button
        android:id="@+id/proxy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Proxy"/>

    <Button
        android:id="@+id/TitanicTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TitanicTextView"/>

    <Button
        android:id="@+id/sample"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ViewPager渐变背景色"/>

    <Button
        android:id="@+id/eucild"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Eucild"/>

    <Button
        android:id="@+id/dialog_silde"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dialog滑动"/>

    <Button
        android:id="@+id/sildingDrawer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="抽屉控件"/>

    <Button
        android:id="@+id/picture"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="相册"/>

</LinearLayout>